{
  "routes": [
    {
      "src": "/[^.]+",
      "dest": "/",
      "headers": {
        "Access-Control-Allow-Credentials": "true",
        "Access-Control-Allow-Origin": [
          "https://goth-ecommerce-website.vercel.app/",
          "https://goth-ecommerce-website.vercel.app/clothes",
          "https://goth-ecommerce-website.vercel.app/user",
          "https://goth-ecommerce-website.vercel.app/user/session",
          "http://localhost:5173/",
          "http://localhost:5173/clothes",
          "http://localhost:5173/user/",
          "http://localhost:5173/user/session"
        ],
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, PATCH, OPTIONS"
      }
    }
  ]
}